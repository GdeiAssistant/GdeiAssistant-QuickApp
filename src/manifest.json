{
    "package": "edu.gdei.gdeiassistant",
    "name": "易小助",
    "versionName": "1.0.0",
    "versionCode": 1,
    "minPlatformVersion": 1020,
    "icon": "/Common/logo.png",
    "features": [
        {
            "name": "system.prompt"
        },
        {
            "name": "system.router"
        },
        {
            "name": "system.shortcut"
        },
        {
            "name": "system.storage"
        },
        {
            "name": "system.fetch"
        }
    ],
    "permissions": [
        {
            "origin": "*"
        }
    ],
    "config": {
        "logLevel": "debug",
        "data": {
            "requestValidateToken": "GdeiAssistantRequest"
        }
    },
    "router": {
        "entry": "Login",
        "pages": {
            "Login": {
                "component": "index"
            },
            "Index": {
                "component": "index"
            }
        }
    },
    "display": {
        "titleBarBackgroundColor": "#f2f2f2",
        "titleBarTextColor": "#414141",
        "menu": true,
        "pages": {
            "Login": {
                "titleBarText": "欢迎使用易小助"
            },
            "Index": {
                "titleBarText": "个人中心"
            }
        }
    }
}